<%- title 'New doclate' -%>

<div class="container-form">
  <%- form_for @doclate, :url => user_doclates_path(@user, :parent_id => @parent) do |f| -%>
    <%= f.error_messages -%>

    <%= render :partial => "form", :locals => {:f => f} -%>
    <div class="buttonrow">
      <%= submit_tag 'Create' -%> or <%= link_to 'Cancel', :back, :class => 'negative' -%>
    </div>
  <%- end -%>
</div>